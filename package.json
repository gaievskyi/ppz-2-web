{
  "name": "ppz-2-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,jsx --fix",
    "format": "prettier --write \"./**/*.{ts,tsx,json}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx, jsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,html,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.4",
    "@tanstack/react-query": "^4.22.0",
    "@tanstack/react-query-devtools": "^4.18.0",
    "@tanstack/react-router": "0.0.1-beta.28",
    "@tanstack/react-router-devtools": "0.0.1-beta.19",
    "@types/node": "^18.11.9",
    "axios": "^1.2.2",
    "better-react-carousel": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.5",
    "react-icons": "^4.6.0",
    "react-router-dom": "^6.6.1",
    "react-router@beta": "link:@types/@tanstack/react-router@beta",
    "react-toastify": "^9.1.1",
    "react-use-cart": "^1.13.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": ">=7",
    "lint-staged": ">=10",
    "prettier": "2.8.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.3"
  }
}
